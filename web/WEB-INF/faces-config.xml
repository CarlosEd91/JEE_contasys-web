<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
 <navigation-rule>
  <from-view-id>/pages/menu.jsp</from-view-id>
  <navigation-case>
   <from-outcome>producto</from-outcome>
   <to-view-id>/pages/catalogo/producto.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>tipoProducto</from-outcome>
   <to-view-id>/pages/catalogo/tipoProducto.jsp</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <application>
  <resource-bundle>
   <base-name>com.hogam.contasys.i18n.contasys</base-name>
   <var>inter</var>
  </resource-bundle>
 </application>
</faces-config>
